{
     "whichprovider": {
        "type": "string",
        "description": "选择调用embedding的provider",
        "options": ["ollama", "openai","baidu"]
      },
    "baidu":{
      "type": "object",
      "description": "百度千帆",
      "items": {
        "api_url": {
        "type": "string",
        "description": "在线api服务地址",
        "default": "https://aip.baidubce.com/rpc/2.0/ai_custom/v1/wenxinworkshop/embeddings"
        },
        "api_key": {
          "type": "string",
          "description": "api_key"
        },
        "secret_key": {
          "type": "string",
          "description": "secret_key",
          "hint": "在调用百度应用使用"
        },
        "embed_model": {
          "type": "string",
          "description": "Embedding模型名称"
        }
      }
    },
    "openai":{
      "type": "object",
      "description": "Openai",
      "items": {
        "api_url": {
        "type": "string",
        "description": "Openai服务地址",
        "default": "https://api.openai.com/v1/embeddings"
        },
        "api_key": {
          "type": "string",
          "description": "api_key"
        },
        "embed_model": {
          "type": "string",
          "description": "Embedding模型名称"
        }
      }
    },
    "ollama":{
      "type": "object",
      "description": "Ollama",
      "items": {
        "api_url": {
        "type": "string",
        "description": "Ollama服务地址",
        "default": "http://localhost:11434"
        },
        "embed_model": {
          "type": "string",
          "description": "Embedding模型名称"
        }
      }
    }
}